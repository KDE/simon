SHELL=/bin/sh
RM=@RM@ -f
prefix=@prefix@
exec_prefix=@exec_prefix@
INSTALL=@INSTALL@

############################################################

all:
	@echo Nothing to build

install: install.man install.man.ja

install.man:
	${INSTALL} -d @mandir@/man1
	for f in *.1; do \
	  @INSTALL_DATA@ $$f @mandir@/man1/$$f; \
	done

install.man.ja:
	${INSTALL} -d @mandir@/man1/ja
	cd ja;
	for f in *.1; do \
	  @INSTALL_DATA@ $$f @mandir@/man1/ja/$$f; \
	done
	cd ..

############################################################

clean:
	@echo Nothing to do

distclean:
	$(RM) Makefile
